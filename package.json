{
	"$schema": "http://json.schemastore.org/package",
	"name": "@oadpoaw/ts-package-template",
	"description": "@oadpoaw/ts-package-template",
	"version": "0.1.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"scripts": {
		"prebuild": "rimraf dist tsconfig.tsbuildinfo",
		"build": "tsc -b && rollup -c scripts/rollup.ts && gen-esm-wrapper dist/index.js dist/index.mjs",
		"lint": "prettier --check \"src/**/*.{ts,tsx}\"",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"v:patch": "npm version patch",
		"v:minor": "npm version minor"
	},
	"devDependencies": {
		"@sapphire/ts-config": "^3.3.1",
		"@types/node": "^17.0.21",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.69.0",
		"rollup-plugin-version-injector": "^1.3.3",
		"typescript": "^4.5.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oadpoaw/ts-package-template.git"
	},
	"author": "oadpoaw",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/oadpoaw/ts-package-template/issues"
	},
	"homepage": "https://github.com/oadpoaw/ts-package-template#readme",
	"engines": {
		"node": ">=v16.0.0",
		"npm": ">=7.0.0"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"keywords": [],
	"publishConfig": {
		"access": "public"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"tabWidth": 4,
		"semi": true,
		"endOfLine": "auto",
		"useTabs": true
	}
}
