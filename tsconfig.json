{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"target": "ESNext",
		"declaration": true,
		"declarationMap": true,
		"lib": [
			"DOM"
		],
		"rootDir": "src",
		"outDir": "dist",
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"experimentalDecorators": true
	},
	"include": [
		"src"
	]
}